@model IEnumerable<Category>
@{
    ViewData["Title"] = "Default";
}

<div class="list-group">


  @foreach (var item in Model)
  {

        <a 
           
           asp-controller="Blog"
           asp-action="Index"
           asp-route-id="@item.Id"
           class="list-group-item"
           > @item.Name</a>



    }



  


</div>

